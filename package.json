{
  "name": "angular-wasm",
  "version": "0.1.0",
  "description": "A collection of components and services for Angular 2+ to demonstrate integration with WebAssembly",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run wasm && ng build --configuration production",
    "wasm": "node scripts/compile-wasm.js && node scripts/wasm-cache-busting.js",
    "start": "ng serve",
    "lint": "ng lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --check . --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boyanio/angular-wasm.git"
  },
  "keywords": [
    "Angular",
    "WebAssembly",
    "JavaScript"
  ],
  "private": false,
  "author": "Boyan Mihaylov",
  "dependencies": {
    "@angular/common": "^14.0.1",
    "@angular/compiler": "^14.0.1",
    "@angular/core": "^14.0.1",
    "@angular/forms": "^14.0.1",
    "@angular/platform-browser": "^14.0.1",
    "@angular/platform-browser-dynamic": "^14.0.1",
    "@angular/router": "^14.0.1",
    "angular2-ladda": "^3.1.1",
    "ladda": "^2.0.1",
    "rxjs": "^7.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "^14.0.1",
    "@angular/compiler-cli": "^14.0.1",
    "@types/node": "^17.0.42",
    "chalk": "^4.1.2",
    "codelyzer": "^6.0.2",
    "prettier": "^2.0.5",
    "tslint": "~6.1.0",
    "typescript": "4.7"
  }
}
